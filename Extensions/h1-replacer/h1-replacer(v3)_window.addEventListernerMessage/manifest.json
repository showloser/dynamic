{
    "manifest_version": 3,
    "name": "Message Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "file:///*"
    ]
  }
  