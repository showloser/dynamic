{
  "manifest_version": 3,
  "name": "Context Menu FIXED",
  "version": "1.0",
  "permissions": ["contextMenus", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
      "file:///*"
  ]
}
