{
    "manifest_version": 3,
    "name": "chrome debug extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting",
        "debugger"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/",
        "file:///*"
    ]
}